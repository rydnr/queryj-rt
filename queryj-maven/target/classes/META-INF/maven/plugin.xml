<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>QueryJ Maven</name>
  <description></description>
  <groupId>org.acmsl.queryj</groupId>
  <artifactId>queryj-maven</artifactId>
  <version>3.0-SNAPSHOT</version>
  <goalPrefix>queryj</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>queryj</goal>
      <description>Executes QueryJ.</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <executePhase>generate-sources</executePhase>
      <implementation>org.acmsl.queryj.tools.maven.QueryJMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>true</threadSafe>
      <parameters>
        <parameter>
          <name>catalog</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The catalog.</description>
        </parameter>
        <parameter>
          <name>externallyManagedFields</name>
          <type>org.acmsl.queryj.tools.maven.ExternallyManagedField[]</type>
          <required>false</required>
          <editable>true</editable>
          <description>The list of external managed fields</description>
        </parameter>
        <parameter>
          <name>grammarFolder</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>The grammar folder.</description>
        </parameter>
        <parameter>
          <name>headerFile</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>The header file.</description>
        </parameter>
        <parameter>
          <name>outputDir</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>The output directory.</description>
        </parameter>
        <parameter>
          <name>sqlXmlFile</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>The sql xml file.</description>
        </parameter>
        <parameter>
          <name>m__aTables</name>
          <type>org.acmsl.queryj.tools.maven.Table[]</type>
          <required>false</required>
          <editable>true</editable>
          <description>The list of tables.</description>
        </parameter>
        <parameter>
          <name>disableCheckthreadAnnotations</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Whether to disable checkthread.org annotations.</description>
        </parameter>
        <parameter>
          <name>disableGenerationTimestamps</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Whether to generate file timestamps.</description>
        </parameter>
        <parameter>
          <name>disableNotNullAnnotations</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Whether to disable NotNull annotations.</description>
        </parameter>
        <parameter>
          <name>driver</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The driver.</description>
        </parameter>
        <parameter>
          <name>encoding</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The file encoding.</description>
        </parameter>
        <parameter>
          <name>grammarName</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The grammar bundle.</description>
        </parameter>
        <parameter>
          <name>grammarSuffix</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The grammar suffix.</description>
        </parameter>
        <parameter>
          <name>jndiDataSource</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The data source.</description>
        </parameter>
        <parameter>
          <name>packageName</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The package name.</description>
        </parameter>
        <parameter>
          <name>password</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The password.</description>
        </parameter>
        <parameter>
          <name>repository</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The repository.</description>
        </parameter>
        <parameter>
          <name>url</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The url.</description>
        </parameter>
        <parameter>
          <name>username</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The user name.</description>
        </parameter>
        <parameter>
          <name>schema</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The schema.</description>
        </parameter>
        <parameter>
          <name>session</name>
          <type>org.apache.maven.execution.MavenSession</type>
          <required>true</required>
          <editable>false</editable>
          <description>The current build session instance. This is used for toolchain manager API calls.</description>
        </parameter>
      </parameters>
      <configuration>
        <encoding implementation="java.lang.String" default-value="${project.build.sourceEncoding}"/>
        <session implementation="org.apache.maven.execution.MavenSession" default-value="${session}"/>
      </configuration>
    </mojo>
  </mojos>
